/* 资讯*/
.news_toggle_icon{ padding:4px 10px; background:url(arrow_down_white.png) center center no-repeat; background-size:20px 20px;}
.news_menu_wrap{  position:fixed; top:0; right:0; left:0; bottom:0; z-index:1000;}
