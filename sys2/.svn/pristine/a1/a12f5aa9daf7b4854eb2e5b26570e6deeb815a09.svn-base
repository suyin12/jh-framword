.service_content{ margin:20px 0;padding:5px 15px 15px;}
.service_content .content{ }
.service_content .title{ font-size:16px; padding-bottom:5px; margin-bottom:10px; border-bottom:1px solid #eee;}
.service_content .title img{width:30px; vertical-align:middle; margin-right:3px;}
.service_content .content{ text-align:center}
.service_content .code{ background:#eee; padding:15px 20px; font-size:26px; margin:10px 0;}
.share_type{ padding:15px 0; display:-webkit-box;}
.share_type a{ display:block; width:33.33%; -webkit-box-flex:1; margin:0 5px; padding:10px 0; text-align:center; color:#fff}
.share_type a img{ width:50%;}
.msg_send_actionsheet{ background:#fff}
.msg_send_actionsheet p{ padding:10px 0;}
.msg_send_actionsheet .weui_actionsheet_menu{ padding:15px 15px 0; text-align:center}
.msg_send_actionsheet input{ border:1px solid #ccc; height:40px; padding:0 5px; width:97%; border-radius:5px;font-size:16px; margin-bottom:15px;}
/* 分享提示 */
.shareTips{ background-color:RGBA(0,0,0,0.6); position:fixed;top:0; right:0;bottom:0;left:0; z-index:10000;}
.shareTips .tipsPic{ width:100%; height:500px; background:url(../../Home/images/share_tips.png?21) no-repeat; position:absolute; top:0; right:0;background-size:100% auto}
.shareTips .tips_friend{ width:100%; height:500px; background:url(../../Home/images/share-friend.png) no-repeat; position:absolute; top:0; right:0;background-size:100% auto}
.shareTips .tips_concern{ width:100%; height:500px; background:url(../../Home/images/share_concern.png) no-repeat; position:absolute; top:0; right:0;background-size:100% auto}
.shareTips .qrcode{ width:70%; height:250px; position:absolute; top:50%; left:15%; text-align:center; color:#fff}
.shareTips .qrcode img{ width:70%;}
/*测算 */
.ca_detail_title{ padding:10px 0; text-align:center; border-bottom:1px solid #ddd; margin-bottom:10px;}
.ca_detail_table th,.ca_detail_table td{ padding:5px; text-align:center}
.ca_detail_table th{ font-size:13px; color:#888; font-weight:normal}
.ca_detail_table td.small{ color:#000}
.ca_detail_table{ width:100%; padding:0;}
.ca_detail_table th{ font-size:14px;}
.ca_detail_table td{ border-bottom:1px dotted #eee; font-size:14px;}
.ca_detail_table tr:last-child td{ border:none}
/* 页码 */
.page_hd{ margin:20px; text-align:center;}
.page_hd a,.page_hd span{ padding:4px 8px; border:1px solid #ddd; margin:0 5px; font-size:12px; border-radius:3px; background:#eee;}
.page_hd .current{ background:#4b9adc; border:1px solid #4b9adc; color:#fff}

/* 快捷弹出 */
.pop_cate{ position:fixed; right:0; left:0; top:0; bottom:0; z-index:1000; background:RGBA(0,0,0,.9); display:none}
.pop_cate .unlogin{ padding:15px 0; text-align:center; color:#fff;}
.pop_cate .unlogin .weui_btn{ width:100px; margin:10px auto;}
.pop_cate_lists{ text-align:center; margin:0 5px 0; display:-webkit-box;}
.pop_cate_lists li{ width:25%; -webkit-box-flex:1;margin:5px;}
.pop_cate_lists .item{ display:block; padding:10px 0; color:#fff; overflow:hidden;}
.pop_cate_lists .item .icon{ width:50%; margin:0 auto; display:block;}
.pop_cate_lists .item .icon img{ width:100%; display:block}
.pop_cate_lists .item .txt{ font-size:14px;}
.pop_bottom_in{-webkit-animation:pop_bottom_in ease-in-out 300ms;animation:pop_bottom_in ease-in-out 300ms;}
.slow_move{-webkit-animation:slow_move ease-in-out 500ms;animation:slow_move ease-in-out 500ms;}
.shake_a_shake{-webkit-animation:shake_a_shake ease-in-out 1s;animation:shake_a_shake ease-in-out 1s;}

/*账户流水*/
.circle_shou,.circle_zhi{border-radius:10px;border:1px solid;color:#fff;display:inline-block;width:17px;height:17px;text-indent:3px}
.circle_shou{background: #a72525;}
.circle_zhi{background: #396;}







@keyframes pop_bottom_in
{
from { transform:translate(0,100%);}
to {transform:translate(0,0);}
}

@-webkit-keyframes pop_bottom_in
{
from {-webkit-transform:translate(0,100%);}
to {-webkit-transform:translate(0,0);}
}
@keyframes slow_move
{
from { transform:translate(0,60px);}
to {transform:translate(0,0);}
}

@-webkit-keyframes slow_move
{
from {-webkit-transform:translate(0,60px);}
to {-webkit-transform:translate(0,0);}
}
@keyframes shake_a_shake
{
0% { transform:scale(0,0)}
25% { transform:scale(1.1,1.1);}
50% { transform:scale(1.0,1.0);}
75% { transform:scale(1.1,1.1);}
100% { transform:scale(1,1);}
}

@-webkit-keyframes shake_a_shake
{
0% { -webkit-transform:scale(0,0)}
25% { -webkit-transform:scale(1.1,1.1);}
50% { -webkit-transform:scale(1.0,1.0);}
75% { -webkit-transform:scale(1.1,1.1);}
100% { -webkit-transform:scale(1,1);}
}

