.qrcode_dialog .close{ position:absolute; top:15px; right:15px; width:30px; height:30px; background:url(close.png) no-repeat; background-size:auto 100%;}
/* 客服页面 */
.help_content{ margin:20px 0; padding:15px 15px 15px;}
.help_content .title{ font-size:16px; padding-bottom:5px; margin-bottom:10px; border-bottom:1px solid #eee;}
.help_content .title img{width:30px; vertical-align:middle; margin-right:3px;}
.help_content .weui_icon_safe:before{ font-size:30px; margin-right:3px;}
