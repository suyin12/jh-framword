


.container,.page{}
body,html{height:100%;-webkit-tap-highlight-color:transparent;}
/*.page,body{background-color:#FBF9FE}*/
.page,body{background-color:#F0F0F0;}
.container{overflow:hidden}
.page{ margin-top:44px;overflow-y:auto;}
.with_foot_page{ margin-bottom:50px;}
a{ color:#333}
a:hover,a:visited{ color:#333}
ul,li{ list-style:none}
.hd{ background:#fff; position:relative}
.hd:before{  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1px;
  border-bottom: 1px solid #D9D9D9;
  -webkit-transform-origin: 0 100%;
  -ms-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  -ms-transform: scaleY(0.5);
  transform: scaleY(0.5);}
.hd:after{  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  border-bottom: 1px solid #D9D9D9;
  -webkit-transform-origin: 0 100%;
  -ms-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  -ms-transform: scaleY(0.5);
  transform: scaleY(0.5);}
.bottom_tips{ font-size:14px; color:#888; text-align:center; padding:0px;}
.top_title{ font-size:16px; color:#444; text-align:center; padding:10px;}
.top_title_al{font-size:14px; color:#444; text-align:left; padding:10px 15px;}
.small{ font-size:12px; color:#888;}
.orange{ color:#F60}
.gray{ color:#888}
.big{ font-size:20px;}
.lit{ font-size:14px;}
.fr{ float:right}
.fl{ float:left;}
.delete_icon{ padding:3px 7px; background:url(delete_icon.png) no-repeat center center; background-size:18px 18px;}
.two_btn_wrap{ display:-webkit-box;}
.two_btn_wrap .weui_btn{ -webkit-box-flex:1; margin:5px 15px; width:50%; display:block}
.status{ padding:2px 4px; font-size:10px; color:#fff; border-radius:3px;}
.status_red{ background:#F30}
.status_green{ background:#2eb106}
.status_yellow{ background:#e87005}
.common_tab{ background:#fff; height:44px; line-height:44px; display:-webkit-box; border-bottom:1px solid #ddd;}
.common_tab a{ -webkit-box-flex:1; width:25%; display:block; text-align:center; border-right:1px solid #ddd; border-bottom:3px solid #fff}
.common_tab a:last-child{ broder:none}
.common_tab a.cur{ border-bottom:3px solid #4b9adc}
.btn{border:none; background-color:#4b9adc; }
.btn:active, .btn.active, .btn.disabled, .btn[disabled]{
	color: #999;
	background-color: #ccc;
	 *background-color: #ccc;
	 cursor:default;
}
.btn.disabled{ text-indent:-10000px; background-image:url(loading_icon.gif?12345); background-position: center center; background-repeat:no-repeat;}
.page_loading{ position:fixed; right:0; left:0; top:0; bottom:0; z-index:1; background:url(loading.gif) no-repeat center center;}
/* header */
.header{ height:44px; background:#4b9adc; padding:0 50px; text-align:center; line-height:44px; position:fixed; top:0; left:0; right:0; z-index:100;}
.header .title{ font-size:18px; color:#fff; font-weight:bold;}
.header .back{ background:url(arrow_left_white.png) no-repeat center center; background-size:auto 60%; position:absolute;left:0; height:44px; width:40px;}
.header .cate{ background:url(cate.png) no-repeat center center; background-size:auto 70%; position:absolute;right:0; height:44px; width:50px;}
/* footer */
.footer{ height:46px; background:#fff; border-top:1px solid #ddd;position:fixed; bottom:0; left:0; right:0; z-index:100; display:-webkit-box;}
.footer a{ -webkit-box-flex:1; display:block; width:25%; text-align:center; font-size:12px; color:#888; padding:28px 0 0;}
.footer a.icon_home{ background:url(home_gray.png) no-repeat center 3px; background-size:22px 22px;}
.footer a.icon_pay{ background:url(pay_gray.png) no-repeat center 3px; background-size:26px 26px;}
.footer a.icon_add{ background:url(bottom_add.png) no-repeat center 3px; background-size:36px 36px; padding:38px 0 0; top:-10px; position:relative;}
.footer a.icon_person{ background:url(person_gray.png) no-repeat center 3px; background-size:26px 26px;}
.footer a.icon_me{ background:url(me_gray.png) no-repeat center 3px; background-size:23px 23px;}
/* banner */
.banner{ width:100%; overflow:hidden; position:relative; margin:0; z-index:10;}
.banner ul{ position:absolute; left:0; top:0; z-index:10; -webkit-animation:mymove 5s infinite;}
.banner ul li{ float:left; display:table-cell; position:relative}
.banner li a{ width:100%; height:100%; display:block;}
.banner li .title{background-color:RGBA(0,0,0,.5); height:30px; color:#fff; line-height:30px; padding-left:10px; position:absolute; left:0; bottom:0; width:100%; z-index:1000;}
.identify{text-align:center; position:absolute; bottom:0; right:0; left:0; z-index:100; height:20px;}
.identify em{ display:inline-block; margin:5px 3px;-webkit-border-radius:100%;-moz-border-radius:100%;
border-radius:100%;width: 10px;height: 10px;background: #fff;}
.identify em.cur{ background-color:#1ab0ee}
