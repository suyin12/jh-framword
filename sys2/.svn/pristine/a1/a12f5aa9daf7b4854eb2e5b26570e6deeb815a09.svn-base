/* 个人中心 */
.user_page{ margin:0 0 50px;}
.user_page_head{ background:#4b9adc url(center_bg.png) no-repeat; background-size:100% 100%; height:180px; position:relative;}
.user_page_head .header{ position:static; background:none;}
.user_page_head .user_head{ width:60px; height:60px; border-radius:30px; margin:10px auto 5px; display:block;}
