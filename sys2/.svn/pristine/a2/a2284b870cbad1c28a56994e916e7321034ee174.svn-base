@keyframes pop_bottom_in
{
from { transform:translate(0,100%);}
to {transform:translate(0,0);}
}

@-webkit-keyframes pop_bottom_in
{
from {-webkit-transform:translate(0,100%);}
to {-webkit-transform:translate(0,0);}
}
@keyframes slow_move
{
from { transform:translate(0,60px);}
to {transform:translate(0,0);}
}

@-webkit-keyframes slow_move
{
from {-webkit-transform:translate(0,60px);}
to {-webkit-transform:translate(0,0);}
}
@keyframes shake_a_shake
{
0% { transform:scale(0,0)}
25% { transform:scale(1.1,1.1);}
50% { transform:scale(1.0,1.0);}
75% { transform:scale(1.1,1.1);}
100% { transform:scale(1,1);}
}

@-webkit-keyframes shake_a_shake
{
0% { -webkit-transform:scale(0,0)}
25% { -webkit-transform:scale(1.1,1.1);}
50% { -webkit-transform:scale(1.0,1.0);}
75% { -webkit-transform:scale(1.1,1.1);}
100% { -webkit-transform:scale(1,1);}
}


